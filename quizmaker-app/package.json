{
	"name": "quizmaker-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "opennextjs-cloudflare build",
		"start": "next start",
		"lint": "next lint",
		"test": "vitest",
		"test:ui": "vitest --ui",
